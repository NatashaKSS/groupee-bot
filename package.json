{
  "name": "groupee-bot",
  "version": "1.0.0",
  "description": "A Telegram bot that fixes group messes",
  "main": "index.js",
  "engines": {
    "node": "8.10"
  },
  "scripts": {
    "start": "npx ntl --descriptions",
    "run-func-local": "serverless invoke local --function serve",
    "start-offline": "serverless offline start --stage dev",
    "deploy-dev": "serverless deploy --stage dev",
    "deploy-func-serve-dev": "serverless deploy function --function serve --stage dev",
    "deploy-prod": "serverless deploy --stage prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NatashaKSS/groupee-bot.git"
  },
  "author": "Natasha Koh Sze Sze",
